<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-configuration PUBLIC
        "-//Hibernate/Hibernate Configuration DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
    <session-factory>
        <property name="connection.url">jdbc:mysql://localhost:3306/BotanyPark?useUnicode=true&amp;characterEncoding=UTF-8</property>
        <property name="connection.driver_class">com.mysql.jdbc.Driver</property>
        <property name="hibernate.connection.username">root</property>
        <property name="hibernate.connection.password">20161024</property>
        <property name="hibernate.dialect">org.hibernate.dialect.MySQLDialect</property>

        <mapping resource="po/UserEntity.hbm.xml"/>
        <!--<mapping class="po.BotanyEntity"/>-->
        <mapping resource="po/BotanyEntity.hbm.xml"/>
        <mapping resource="po/DistanceEntity.hbm.xml"/>
        <!--<mapping class="po.DistanceEntity"/>-->
        <mapping resource="po/EcosEntity.hbm.xml"/>
        <!--<mapping class="po.EcosEntity"/>-->
        <!--<mapping class="po.SceneryEntity"/>-->
        <mapping resource="po/SceneryEntity.hbm.xml"/>

    </session-factory>
</hibernate-configuration>