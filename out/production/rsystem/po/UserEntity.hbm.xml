<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="po.UserEntity" table="user" schema="BotanyPark">
        <id name="id">
            <column name="id" sql-type="int(11)" not-null="true"/>
        </id>
        <property name="password">
            <column name="password" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="name">
            <column name="name" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="tap">
            <column name="tap" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>